<%= render 'shared/banner' %>
<h1>OfficeSpaces#index</h1>
<% @office_spaces.each do |office_space| %>
<h2><%= link_to office_space.name, office_space_path(office_space) %></h2>
<h3><%= office_space.address %></h3>
<h3> Description</h3>
<%= cl_image_tag office_space.photo, height: 300, width: 400, crop: :fill %>
<p>
  <%=  office_space.description%>
</p>
<h3>Price :</h3>
<p>
  <%= office_space.price %> USD
</p>
<% end %>
<hr>
<h3><%= link_to "Add a new office space", new_office_space_path %></h3>
