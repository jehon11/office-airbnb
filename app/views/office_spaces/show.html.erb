
<!-- office photo -->
<div class="photo-container">
  <div class="row big-box">
    <div class="grid">
      <div class="element1">
        <%= @office_space.photo %>
      </div>
      <div class="element2">
        <%= @office_space.photo %>
      </div>
      <div class="element3">
        <%= @office_space.photo %>
      </div>
    </div>
  </div>
</div>

<!-- office info -->
<div class="container">
  <div class="row">
    <div class="col-md-8"> <!-- left box -->
      <h3><%= @office_space.name %></h3>
      <ul id="list">
        <li><%= @office_space.address %></li>
        <br>
        <li><%= @office_space.description %></li>
        <br>
        <li>Nearest Station: <%= @office_space.nearest_station %></li>
        <br>
        <li>Host: <%= @office_space.owner %></li>
      </ul>
    </div>
    <div class="col-md-4" style="text-align:right; margin-top:60px;"> <!-- right box -->
      <h4><%= @office_space.price %>$ </h4>
      <a href="#" class="btn-treehouse pull-right">BOOK</a>
    </div>
  </div>
</div>
